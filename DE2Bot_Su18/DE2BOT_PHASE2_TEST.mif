           2E8 : 01F4;  -- FFast:    DW 500       ; 500 is almost max speed (511 is max)
           2E9 : FE0C;  -- RFast:    DW -500
           2EA : 008C;  -- MinBatt:  DW 140       ; 14.0V - minimum safe battery voltage
           2EB : 1190;  -- I2CWCmd:  DW &H1190    ; write one i2c byte, read one byte, addr 0x90
           2EC : 0190;  -- I2CRCmd:  DW &H0190    ; write nothing, read one byte, addr 0x90
  END;
